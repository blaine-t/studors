  "scripts": {
    "clean": "npx rimraf dist/* dev/docker/app/*",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "minify": "npx ts-node ./dev/scripts/minify",
    "prebuild": "npm-run-all clean lint-and-fix minify",
    "build": "npx tsc",
    "postbuild": "npx ts-node ./dev/scripts/build",
    "docker-setup": "npx ts-node ./dev/scripts/docker",
    "docker-build": "npm-run-all build docker-setup",
    "prod": "node .",
    "dev": "npx nodemon src/index.ts",
    "dev:debug": "nodemon --inspect src/index.ts",
    "css-dev": "npx ts-node ./dev/scripts/css"
  },
  "dependencies": {
    "body-parser": "^2.2.1",
