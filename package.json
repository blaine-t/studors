    "@types/pg": "^8.15.6",
    "@types/shelljs": "^0.8.17",
    "@types/uglifycss": "^0.0.11",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "ejs-lint": "^2.0.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "html-minifier": "^4.0.0",
    "nodemon": "^3.1.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.7.3",
    "shelljs": "^0.10.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
